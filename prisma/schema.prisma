
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Food {
  id String @id @default(uuid())
  name String
  description String
  image String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}